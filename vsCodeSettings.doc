{
    "window.zoomLevel": 1,
    "editor.fontSize": 15,
    "editor.lineHeight": 25,
    "editor.letterSpacing": 0.5,
    "files.trimTrailingWhitespace": true,
    "editor.cursorWidth": 3,
    "workbench.colorTheme": "Cobalt2",
    "editor.renderWhitespace": "all",
    "terminal.integrated.shell.windows": "C:\\Program Files\\Git\\bin\\bash.exe",
    "highlight-matching-tag.styles":  {
        "opening": {
          "name": {
            "underline": "yellow"
          }
        }
      },
      "git.enableSmartCommit": true,
      "terminal.explorerKind": "external",
      "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
      },
      "workbench.iconTheme": "material-icon-theme",
      "yaml.schemas": {
        "file:///c%3A/Users/MC/.vscode/extensions/atlassian.atlascode-2.8.5/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
      },
      "atlascode.jira.jqlList": [
        {
          "id": "7a66ffd7-eead-4cc3-8b30-ebf83bbf6273",
          "enabled": true,
          "name": "My liyeplimal Issues",
          "query": "assignee = currentUser() AND resolution = Unresolved ORDER BY lastViewed DESC",
          "siteId": "e45c0f8b-ab5e-4947-a932-b767e5f263dd",
          "monitor": true
        }
      ],
      "atlascode.jira.lastCreateSiteAndProject": {
        "siteId": "e45c0f8b-ab5e-4947-a932-b767e5f263dd",
        "projectKey": "HAD"
      },
      "better-comments.tags": [

        {
          "tag": "!",
          "color": "#FF2D00",
          "strikethrough": false,
          "underline": false,
          "backgroundColor": "transparent",
          "bold": false,
          "italic": false
        },
        {
          "tag": "?",
          "color": "#3498DB",
          "strikethrough": false,
          "underline": false,
          "backgroundColor": "#ca3433",
          "bold": false,
          "italic": false
        },
        {
          "tag": "//",
          "color": "#474747",
          "strikethrough": true,
          "underline": false,
          "backgroundColor": "transparent",
          "bold": false,
          "italic": false
        },
        {
          "tag": "todo",
          "color": "#FF8C00",
          "strikethrough": false,
          "underline": false,
          "backgroundColor": "transparent",
          "bold": false,
          "italic": false
        },
        {
          "tag": "*",
          "color": "#FF8C00",
          "strikethrough": false,
          "underline": false,
          "backgroundColor": "transparent",
          "bold": false,
          "italic": false
        }
      ],
      "editor.wordWrap": "on",
      "javascript.updateImportsOnFileMove.enabled": "always",
      "editor.fontFamily": "cascadia code",
      // "editor.fontFamily": "Fira Code",
      // "editor.fontFamily": "Operator Mono, Menlo, Monaco, 'Courier New', monospace",
      "editor.fontLigatures": true,
      "editor.fontWeight": "400",
      "cSpell.language": "en,en-US",
      "cSpell.userWords": [
        "فصولا"
      ],
      "window.title": "${dirty}${activeEditorShort}${separator}${rootName}${separator}${rootPath}",
      "editor.cursorBlinking": "expand",
      "editor.cursorSmoothCaretAnimation": true,
      "terminal.integrated.cursorBlinking": true,
      "terminal.integrated.cursorStyle": "line",
      "terminal.integrated.cursorWidth": 3,
}

